@model IEnumerable<TeacherPortal.Models.Qualification>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Teacher.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.University_School)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YOP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Specification)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Teacher.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.University_School)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YOP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Specification)
        </td>
        
    </tr>
}

</table>
